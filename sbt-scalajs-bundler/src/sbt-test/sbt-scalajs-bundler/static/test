-$ exists target/scala-2.11/static-fastopt-bundle.js
> fastOptJS::webpack
$ exists target/scala-2.11/static-fastopt-bundle.js
> html index.html

-$ exists target/scala-2.11/static-opt-bundle.js
> fullOptJS::webpack
$ exists target/scala-2.11/static-opt-bundle.js
> html index-prod.html

# Disabling source maps in Scala.js also disables it for scalajs-bundler
> clean
> set emitSourceMaps in fastOptJS := false
> fastOptJS::webpack
-$ exists target/scala-2.11/static-fastopt-bundle.js.map

# emitSourceMaps can be defined just for the webpack task
> set emitSourceMaps in (webpack in fullOptJS) := false
> fullOptJS::webpack
$ exists target/scala-2.11/static-opt.js.map
-$ exists target/scala-2.11/static-opt-bundle.js.map
